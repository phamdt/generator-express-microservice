{
  "name": "<%= modelName.toLowerCase() %>-service",
  "author": "Rakuten Marketing",
  "version": "1.0.0",
  "description": "A microservice for <%= modelName.toLowerCase() %> CRUD",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "serve": "./node_modules/.bin/nodemon -V -w app/ index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --opts 'test/mocha/mocha.opts' 'test/mocha/**/*.js'",
    "garden": "./node_modules/.bin/plus.garden test",
    "test-watch": "./node_modules/.bin/nodemon --watch app --watch test -x \"npm test\"",
    "migration": "./node_modules/.bin/knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.private.linksynergy.com:SD/<%= modelName.toLowerCase() %>-service.git"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "body-parser": "^1.14.1",
    "bluebird": "^3.0.5",
    "cors": "^2.7.1",
    "create-error": "^0.3.1",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "express-unless": "^0.3.0",
    "knex": "^0.8.6",
    "morgan": "^1.6.1",
    "pg": "^4.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "faker": "^3.0.1",
    "knex-cleaner": "^1.1.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.7.1",
    "plus.garden": "*",
    "plus.garden.api": "*",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "sinon-stub-promise": "^0.1.0"
  },
  "optionalDependencies": {

  },
  "engines": {
    "node": "0.12.7"
  }
}
