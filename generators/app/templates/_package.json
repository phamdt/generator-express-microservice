{
  "name": "<%= modelName %>-service",
  "author": "Rakuten Marketing",
  "version": "1.0.0",
  "description": "A microservice that does cool stuff",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "serve": "./node_modules/.bin/nodemon -V -w app/ index.js",
    "test": "./node_modules/.bin/mocha 'test/**/*.js'",
    "test-watch": "./node_modules/.bin/nodemon --watch app --watch test -x \"npm test\"",
    "garden": "cd ./garden && ./garden/node garden.js test -d"
  },
  "repository": {
    "type": "git",
    "url": "git@github.private.linksynergy.com:yourusername/<%= modelName %>-service.git"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "knex": "^0.8.6",
    "morgan": "^1.6.1",
    "pg": "^4.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "knex-cleaner": "^1.1.1",
    "mocha": "^2.3.3",
    "nodemon": "^1.7.1",
    "plus.garden": "*",
    "plus.garden.api": "*"
  },
  "engines": {
    "node": "0.12.7"
  }
}
